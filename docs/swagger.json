{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "Exchange rate tracking service for UnionPay CNY/JPY rates\n\nThis API provides access to historical and current exchange rates\nbetween Chinese Yuan (CNY) and Japanese Yen (JPY) from UnionPay.",
        "title": "RateFlow API",
        "contact": {
            "name": "API Support",
            "url": "https://github.com/tyokyo320/rateflow",
            "email": "support@example.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.1.5"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/rates": {
            "get": {
                "description": "Retrieves the exchange rate for a given currency pair on a specific date",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "rates"
                ],
                "summary": "Get exchange rate for a specific date",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Currency pair (e.g., CNY/JPY, CNYJPY, or CNY-JPY)",
                        "name": "pair",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Date in YYYY-MM-DD format (e.g., 2025-01-15)",
                        "name": "date",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success response with rate data",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Bad request error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Rate not found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/api/rates/latest": {
