        "/api/rates/list": {
            "get": {
                "description": "Retrieves a paginated list of exchange rates, optionally filtered by currency pair",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "rates"
                ],
                "summary": "List exchange rates with pagination",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Currency pair filter (e.g., CNY/JPY, CNYJPY, or CNY-JPY)",
                        "name": "pair",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "Page number (default: 1)",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 20,
                        "description": "Items per page (default: 20, max: 100)",
                        "name": "pageSize",
                        "in": "query"
                    }
                ],
                "responses": {
